{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier", "jest"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": "off",
    "react/jsx-one-expression-per-line": "off",
    "no-param-reassign": "off"
  },
  "env": {
    "browser": true,
    "jest/globals": true
  }
}
